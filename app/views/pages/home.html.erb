<h1>Pages#home</h1>
<p>Find me in app/views/pages/home.html.erb</p>
<div>
<h3>Welcome the the Manticore!</h3>
<% if current_member %>
    <%= link_to "Sign out", destroy_member_session_path, method: :delete, :data {turbo_method: :delete }%>
    <%= link_to "Edit Membership", edit_member_registration_path %>
<% else %>
    <%= link_to "Sign Up", new_member_registration_path %>
    <%= link_to "Sign In", new_member_session_path %>
<% elsif current_admin %>
    <%= link_to "Sign out", destroy_admin_session_path, method: :delete, :data {turbo_method: :delete }%>
    <%= link_to "Edit ", edit_admin_registration_path %>
<% else %>
    <%= link_to "Sign Up", new_admin_registration_path %>
    <%= link_to "Sign In", new_admin_session_path %>
<% end %>